bool runningText() {
  static int column_pos = 0;
  static int text_width = 0;

  if (!shouldStopAP) {
    column_pos = column_pos - 3;
    if (column_pos < (0 - (text_width + 1))) {
      column_pos = display.width();
      text_width = ((text_length * 11)*3)+40;
      setTextRun();
    }
  }

  display.setTextSize(2);
  printMessage(true, title_pos, 0, title, false);
  display.setTextSize(5);
  printMessage(false, column_pos, 18, text, true);
  
  return shouldStopAP;
}


